{
  "name": "nine-limit",
  "version": "1.0.0",
  "description": "Lunar calendar conversion tool whose precision is up to minute unit.",
  "main": "dist/nine-limit.min.js",
  "scripts": {
    "test": "jest",
    "cov": "jest --coverage",
    "build": "npm run build:clean && npm run build:babel && npm run build:browserify && npm run build:minify",
    "build:clean": "rimraf lib/*",
    "build:babel": "babel src -d lib",
    "build:browserify": "browserify lib/nine-limit.js > dist/nine-limit.bundle.js",
    "build:minify": "minify dist/nine-limit.bundle.js > dist/nine-limit.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yo8568/nine-limit.git"
  },
  "keywords": [
    "lunar-calendar",
    "nine-limit"
  ],
  "author": "Kenny Chang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yo8568/nine-limit/issues"
  },
  "homepage": "https://github.com/yo8568/nine-limit#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.7.1"
  },
  "dependencies": {
    "browserify": "^16.2.3",
    "lunar-calendar": "^0.1.4",
    "minify": "^4.1.1",
    "moment": "^2.24.0"
  }
}
